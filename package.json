{
  "name": "project-management-api",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "build": "npm install && npm run db:migrate",
    "dev": "nodemon src/server.js",
    "db:migrate": "sequelize-cli db:migrate --config src/config/config.js --migrations-path src/migrations",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0"
  },
  "engines": {
    "node": "18.17.1"
  }
}
