{
  "name": "project-management-api",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "build": "npm install && npm run db:migrate",
    "dev": "nodemon src/server.js",
    "db:migrate": "sequelize-cli db:migrate --config src/config/config.js --migrations-path src/migrations",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "engines": {
    "node": "18.17.1"
  }
}
